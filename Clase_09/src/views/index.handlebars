<div>
    <h1>Hola, {{user.name}}</h1>
    {{#if isAdmin}}
    {{!-- rederizar la lista de comidas --}}
    {{#each food}}
    <div>
        <p>Nombre: {{this.name}}</p>
        <p>Precio: {{this.price}}</p>
    </div>
    {{/each}}

    {{else}}
    <p>No tienes permisos para ver el listado de comidas!!</p>
    {{/if}}
</div>
<script src="/js/index.js"></script>